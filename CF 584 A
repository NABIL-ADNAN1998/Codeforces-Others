#include <bits/stdc++.h>
using ll = long long int;
using namespace std;

int main()
{
    ll n, t, temp, p;
    cin >> n >> t;
    if (n < 2 && t == 10)
    {
        cout << "-1" << endl;
        return 0;
    }
    else
    {
        if (t == 10)
        {
            cout << "1";
            for (ll i = 0; i < n - 1; i++)
            {
                cout << "0";
            }
        }
        else
        {
            for (ll i = 0; i < n; i++)
            {
                cout << t;
            }
        }
    }
}
