#include <bits/stdc++.h>
using ll = long long int;
using namespace std;

int main()
{
    ll n, p, q, temp;
    cin >> n >> p;
    set<int> s;
    for (ll i = 0; i < p; i++)
    {
        cin >> temp;
        s.insert(temp);
    }
    cin >> q;
    for (ll i = 0; i < q; i++)
    {
        cin >> temp;
        s.insert(temp);
    }
    if (s.size() == n)
    {
        cout << "I become the guy." << endl;
        return 0;
    }
    else
    {
        cout << "Oh, my keyboard!" << endl;
        return 0;
    }
}
