#include<bits/stdc++.h>
using namespace std;
int main()
{
 
    int t;
    cin>>t;
    while(t--)
    {
 
        long long int n,i,j=0,k;
        cin>>n;
        int a[n];
        for(i=0; i<n; i++)
        {
            cin>>a[i];
        }
        for(i=2; i<n; i++)
        {
            if((a[0]+a[1])<=a[i])
            {
                cout<<"1 "<<"2 "<<i+1<<endl;
                j++;
                break;
            }
            else
            {
                continue;
            }
        }
        if(j==0)
        {
            cout<<"-1"<<endl;
 
        }
    }
}
