#include <bits/stdc++.h>
using namespace std;
using ll = long long int;

void solve()
{
    string s;
    cin >> s;
    ll n1, n2;
    n1 = (s[0] - '0') + (s[1] - '0') + (s[2] - '0');
    n2 = (s[3] - '0') + (s[4] - '0') + (s[5] - '0');
    if (n1 == n2)
    {
        cout << "YES" << endl;
    }
    else
    {
        cout << "NO" << endl;
    }
}

int main()
{
    ll test;
    cin >> test;

    while (test--)
    {
        solve();
    }

    // solve();
}
