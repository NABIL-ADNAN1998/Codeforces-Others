//solution of codeforces 677 A
#include <bits/stdc++.h>
#include <utility>
#include <vector>
using ll = long long int;
using namespace std;
int main()
{
    ll n, h, count = 0;
    cin >> n >> h;
    ll mylist[n];
    for (ll i = 0; i < n; i++)
    {
        cin >> mylist[i];
    }

    for (ll i = 0; i < n; i++)
    {
        if (mylist[i] > h)
        {
            count += 2;
        }
        else
        {
            count++;
        }
    }
    cout << count << endl;
}
