#include <bits/stdc++.h>
#include <utility>
#include <vector>
using ll = long long int;
using namespace std;
int main()
{
    ll n, dollar = 0;
    cin >> n;
    ll list[n];
    cin >> list[0];
    dollar = list[0];
    for (ll i = 1; i < n; i++)
    {
        cin >> list[i];
        if (list[i] > dollar)
        {
            dollar = list[i];
        }
    }
    cout << dollar << endl;
    return 0;
}
