#include <bits/stdc++.h>
using namespace std;
using ll = long long int;

void solve()
{
    ll n;
    cin >> n;
    if (n == 1 || n == 2 || n == 4 || n == 5 || n == 8 || n == 11)
    {
        cout << "NO" << endl;
    }
    else
    {
        cout << "YES" << endl;
    }
}

int main()
{
    ll test;
    cin >> test;

    while (test--)
    {
        solve();
    }

    // solve();
}
