#include <bits/stdc++.h>
#include <algorithm>
#include <stdlib.h>
using ll = long long int;
using namespace std;
int main()
{
    string s1, s2;
    cin >> s1 >> s2;
    if (s1 == s2)
    {
        cout << "-1";
        return 0;
    }
    else
    {
        cout << max(s1.size(), s2.size());
        return 0;
    }
}
